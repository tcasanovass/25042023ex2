     -->

    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Award -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Award">
        <rdfs:comment>Represents the awards that could be awarded to a Wine or to a Winnery.</rdfs:comment>
        <rdfs:sameAs rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://dbpedia.org/ontology/Award</rdfs:sameAs>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Certification -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Certification">
        <rdfs:comment>Represents certifications that in our domain can be holded by Wines, Winneries or Grapes.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Cherry -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Cherry">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Wood"/>
        <rdfs:comment>Represents different types of cherry wood</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Country -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Country">
        <rdfs:comment>Represents the different countries of the world</rdfs:comment>
        <rdfs:sameAs rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://dbpedia.org/ontology/country</rdfs:sameAs>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#FortifiedWine -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#FortifiedWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Wine"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#NonFortifiedWine"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Grape -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Grape">
        <rdfs:comment>Represenets actual grapes that may be used, exclusively or in part, to produce n different wines. There could be instances not used for any wines too.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#GrapeVariety -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#GrapeVariety">
        <rdfs:comment>Represents the different grape varieties.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#HighAcidityWine -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#HighAcidityWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Wine"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Material -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Material">
        <rdfs:comment>Represents the root for all materials</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Metal -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Metal">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Material"/>
        <rdfs:comment>Subclass of material that represents metallic alloys, in our domain used to represent metallic materials used in wine production.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#NonFortifiedWine -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#NonFortifiedWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Wine"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#NonSparklingWine -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#NonSparklingWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Wine"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#SparklingWine"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Oak -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Oak">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Wood"/>
        <rdfs:comment>Represents different types of oak wood</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Pine -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Pine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Wood"/>
        <rdfs:comment>Represents different types of pine wood</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#PremiumWine -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#PremiumWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Wine"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#RedWine -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#RedWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Wine"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Region -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Region">
        <rdfs:comment>Represents different regions in a country or spanned in multiple countries.</rdfs:comment>
        <rdfs:sameAs rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://dbpedia.org/ontology/Region</rdfs:sameAs>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#RoseWine -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#RoseWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Wine"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#SparklingWine -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#SparklingWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Wine"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Steel -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Steel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Metal"/>
        <rdfs:comment>Represents steel types, in our domain used for steel alloys used in wine aging ie.: steel 304, 316,...</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#WhiteWine -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#WhiteWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Wine"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Wine -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Wine">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#hasAcidity"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#hasBody"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#hasRating"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#hasAcidity"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">3</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#hasBody"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">5</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#hasRating"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">5</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment>Represents different wines produced by winneries.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Winery -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Winery">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#locatedInCountry"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Country"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment>Represents the different wineries which produce wines.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Wood -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Wood">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Material"/>
        <rdfs:comment>Represents kinds of wood, in our domain represents wood types used to make barrels for aging wine.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/admin2/ontologies/2023/3/wine#YoungWine -->

    <owl:Class rdf:about="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#YoungWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/admin2/ontologies/2023/3/wine#Wine"/>
    </owl:Class>
    


